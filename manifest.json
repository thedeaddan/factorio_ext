{
  "manifest_version": 2,
  "name": "Factorio Mods Downloader",
  "version": "1.0",
  "description": "Download Factorio mods from re146.dev",
  "content_scripts": [
    {
      "matches": [
        "https://mods.factorio.com/mod/*"
      ],
      "js": [
        "jquery.min.js", "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}
